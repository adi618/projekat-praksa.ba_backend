{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint-config-airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "import/extensions": 0,
        "quotes": "off",
        "consistent-return": "off",
        "eqeqeq": "off",
        "no-underscore-dangle": "off"
    }
}
